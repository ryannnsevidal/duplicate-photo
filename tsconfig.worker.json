{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"outDir": "dist",
		"rootDir": "src",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"forceConsistentCasingInFileNames": true,
		"declaration": false,
		"sourceMap": false
	},
	"include": ["src/worker.ts"],
	"exclude": ["node_modules", "pix-dupe-detect-main/**"]
}