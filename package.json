{
  "name": "duplicate-photo-detector",
  "version": "1.0.0",
  "description": "Production-ready duplicate photo detection with Supabase, perceptual hashing, and cloud storage",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "preview": "vite preview",
    "start": "vite preview --port 8080",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:auth": "playwright test auth",
    "test:e2e:upload": "playwright test upload",
    "test:e2e:admin": "playwright test admin",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:all": "npm run test:unit && npm run test:e2e",
    "playwright:install": "playwright install",
    "playwright:install-deps": "playwright install-deps",
    "test:e2e:install": "npx playwright install",
    "db:generate-types": "supabase gen types typescript --local > src/types/supabase.ts",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase migration up",
    "db:seed": "supabase db seed",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "clean": "rm -rf node_modules dist .turbo",
    "clean:deps": "rm -rf node_modules package-lock.json && npm install",
    "test:smoke": "playwright test --grep @smoke"
  },
