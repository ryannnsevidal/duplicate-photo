{
  "name": "duplicate-photo-detector",
  "version": "1.0.0",
  "description": "Production-ready duplicate photo detection with Supabase, perceptual hashing, and cloud storage",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node dist/worker.js",
    "build": "tsc -p tsconfig.worker.json",
    "build:worker": "tsc -p tsconfig.worker.json",
    "build:dev": "echo noop",
    "build:prod": "echo noop",
    "preview": "echo noop",
    "start": "node dist/worker.js",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:auth": "playwright test auth",
    "test:e2e:upload": "playwright test upload",
    "test:e2e:admin": "playwright test admin",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:all": "npm run test:unit && npm run test:e2e",
    "playwright:install": "playwright install",
    "playwright:install-deps": "playwright install-deps",
    "test:e2e:install": "npx playwright install",
    "db:generate-types": "supabase gen types typescript --local > src/types/supabase.ts",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase migration up",
    "db:seed": "supabase db seed",
    "migrate": "node scripts/migrate.mjs",
    "seed": "node scripts/seed.mjs",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "clean": "rm -rf node_modules dist .turbo",
    "clean:deps": "rm -rf node_modules package-lock.json && npm install",
    "test:smoke": "playwright test --grep @smoke"
  },
  "dependencies": {
    "dotenv": "^17.2.1",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@playwright/test": "^1.54.2",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.17.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.47",
    "supabase": "^1.200.3",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vitest": "^2.1.8",
    "prettier": "^3.3.3",
    "pg": "^8.12.0"
  },
  "keywords": [
    "duplicate-detection",
    "supabase",
    "react",
    "typescript",
    "image-processing",
    "perceptual-hash",
    "file-upload",
    "cloud-storage"
  ],
  "author": "Duplicate Photo Detection Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
